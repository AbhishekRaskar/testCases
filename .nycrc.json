{
  "all": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "test/**/*",
    "build/**/*",
    "coverage/**/*",
    "node_modules/**/*",
    "webpack.config.js",
    "src/utils/projectConfig.json"
  ],
  "reporter": [
    "text",
    "text-summary",
    "lcov",
    "html"
  ],
  "report-dir": "./coverage",
  "check-coverage": false,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "cache": true,
  "instrument": true,
  "sourceMap": false,
  "produce-source-map": false
}
